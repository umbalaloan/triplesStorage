###  http://www.co-ode.org/ontologies/pizza/pizza.owl#CheeseTopping

:CheeseTopping rdf:type owl:Class ;
               
               rdfs:label "CoberturaDeQueijo"@pt ;
               
               rdfs:subClassOf :PizzaTopping ;
               
               owl:disjointWith :FishTopping ,
                                :FruitTopping ,
                                :HerbSpiceTopping ,
                                :MeatTopping ,
                                :NutTopping ,
                                :SauceTopping ,
                                :VegetableTopping .



###  http://www.co-ode.org/ontologies/pizza/pizza.owl#CheeseyPizza

:CheeseyPizza rdf:type owl:Class ;
              
              rdfs:label "PizzaComQueijo"@pt ;
              
              owl:equivalentClass [ rdf:type owl:Class ;
                                    owl:intersectionOf ( :Pizza
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :hasTopping ;
                                                           owl:someValuesFrom :CheeseTopping
                                                         ]
                                                       )
                                  ] ;
              
              rdfs:comment "Any pizza that has at least 1 cheese topping."@en .



###  http://www.co-ode.org/ontologies/pizza/pizza.owl#CheeseyVegetableTopping

:CheeseyVegetableTopping rdf:type owl:Class ;
                         
                         rdfs:label "CoberturaDeQueijoComVegetais"@pt ;
                         
                         rdfs:subClassOf :CheeseTopping ,
                                         :VegetableTopping ;
                         
                         rdfs:comment "This class will be inconsistent. This is because we have given it 2 disjoint parents, which means it could never have any members (as nothing can simultaneously be a CheeseTopping and a VegetableTopping). NB Called ProbeInconsistentTopping in the ProtegeOWL Tutorial."@en .



###  http://www.co-ode.org/ontologies/pizza/pizza.owl#ChickenTopping

:ChickenTopping rdf:type owl:Class ;
                
                rdfs:label "CoberturaDeFrango"@pt ;
                
                rdfs:subClassOf :MeatTopping ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasSpiciness ;
                                  owl:someValuesFrom :Mild
                                ] ;
                
                owl:disjointWith :HamTopping ,
                                 :HotSpicedBeefTopping ,
                                 :PeperoniSausageTopping .



###  http://www.co-ode.org/ontologies/pizza/pizza.owl#Country

:Country rdf:type owl:Class ;
         
         rdfs:label "Pais"@pt ;
         
         owl:equivalentClass [ rdf:type owl:Class ;
                               owl:intersectionOf ( :DomainConcept
                                                    [ rdf:type owl:Class ;
                                                      owl:oneOf ( :America
                                                                  :Italy
                                                                  :England
                                                                  :France
                                                                  :Germany
                                                                )
                                                    ]
                                                  )
                             ] ;
         
         rdfs:comment "A class that is equivalent to the set of individuals that are described in the enumeration - ie Countries can only be either America, England, France, Germany or Italy and nothing else. Note that these individuals have been asserted to be allDifferent from each other."@en .



