<http://www.co-ode.org/ontologies/pizza/pizza.owl> rdf:type owl:Ontology ;
                                                   
                                                   owl:versionInfo "version 1.5"^^xsd:string ;
                                                   
                                                   rdfs:comment "An example ontology that contains all constructs required for the various versions of the Pizza Tutorial run by Manchester University (see http://www.co-ode.org/resources/tutorials/)"@en ;
                                                   
                                                   owl:versionInfo "v.1.4. Added Food class (used in domain/range of hasIngredient), Added several hasCountryOfOrigin restrictions on pizzas, Made hasTopping invers functional"@en ,
                                                                   "v.1.5. Removed protege.owl import and references. Made ontology URI date-independent"@en .


#################################################################
#
#    Object Properties
#
#################################################################


###  http://www.co-ode.org/ontologies/pizza/pizza.owl#hasBase

:hasBase rdf:type owl:FunctionalProperty ,
                  owl:InverseFunctionalProperty ,
                  owl:ObjectProperty ;
         
         rdfs:domain :Pizza ;
         
         rdfs:range :PizzaBase ;
         
         rdfs:subPropertyOf :hasIngredient ;
         
         owl:inverseOf :isBaseOf .



###  http://www.co-ode.org/ontologies/pizza/pizza.owl#hasCountryOfOrigin

:hasCountryOfOrigin rdf:type owl:ObjectProperty .



###  http://www.co-ode.org/ontologies/pizza/pizza.owl#hasIngredient

:hasIngredient rdf:type owl:ObjectProperty ,
                        owl:TransitiveProperty ;
               
               rdfs:comment "NB Transitive - the ingredients of ingredients are ingredients of the whole"@en ;
               
               rdfs:domain :Food ;
               
               rdfs:range :Food ;
               
               owl:inverseOf :isIngredientOf .



###  http://www.co-ode.org/ontologies/pizza/pizza.owl#hasSpiciness

:hasSpiciness rdf:type owl:FunctionalProperty ,
                       owl:ObjectProperty ;
              
              rdfs:comment "A property created to be used with the ValuePartition - Spiciness."@en ;
              
              rdfs:range :Spiciness .



###  http://www.co-ode.org/ontologies/pizza/pizza.owl#hasTopping

:hasTopping rdf:type owl:InverseFunctionalProperty ,
                     owl:ObjectProperty ;
            
            rdfs:comment "Note that hasTopping is inverse functional because isToppingOf is functional"@en ;
            
            rdfs:domain :Pizza ;
            
            rdfs:range :PizzaTopping ;
            
            rdfs:subPropertyOf :hasIngredient ;
            
            owl:inverseOf :isToppingOf .



###  http://www.co-ode.org/ontologies/pizza/pizza.owl#isBaseOf

:isBaseOf rdf:type owl:FunctionalProperty ,
                   owl:InverseFunctionalProperty ,
                   owl:ObjectProperty ;
          
          rdfs:range :Pizza ;
          
          rdfs:domain :PizzaBase ;
          
          rdfs:subPropertyOf :isIngredientOf .



###  http://www.co-ode.org/ontologies/pizza/pizza.owl#isIngredientOf

:isIngredientOf rdf:type owl:ObjectProperty ,
                         owl:TransitiveProperty ;
                
                rdfs:comment "The inverse property tree to hasIngredient - all subproperties and attributes of the properties should reflect those under hasIngredient."@en ;
                
                rdfs:domain :Food ;
                
                rdfs:range :Food .



###  http://www.co-ode.org/ontologies/pizza/pizza.owl#isToppingOf

:isToppingOf rdf:type owl:FunctionalProperty ,
                      owl:ObjectProperty ;
             
             rdfs:comment "Any given instance of topping should only be added to a single pizza (no cheap half-measures on our pizzas)"@en ;
             
             rdfs:range :Pizza ;
             
             rdfs:domain :PizzaTopping ;
             
             rdfs:subPropertyOf :isIngredientOf .





